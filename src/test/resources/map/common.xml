<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="common">
	<!--2013-11-27 by liusan.dyf-->
	<sql id="sql">
		$value$
	</sql>
	
	<statement id="getObject" parameterClass="String" resultClass="Object">
		<include refid="sql" />
	</statement>
	
	<statement id="getNull" parameterClass="String">
		<include refid="sql" />
	</statement>
	
	<statement id="getList" parameterClass="String" resultClass="java.util.HashMap" remapResults="true">
		<include refid="sql" />
	</statement>
</sqlMap>